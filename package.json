    "@babel/runtime": "^7.1.2",
    "@themost/cli": "^2.1.8",
    "@types/express": "^4.17.2",
    "@types/jasmine": "^3.5.0",
    "@types/lodash": "^4.14.116",
    "@types/passport": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "es6-promisify": "^6.0.2",
    "eslint": "^5.6.1",
    "jasmine": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "node-fetch": "^2.6.0",
    "nodemon": "^1.19.0",
    "rimraf": "^2.6.3",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.4"
