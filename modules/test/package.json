{
  "name": "@themost/test",
  "version": "1.0.0",
  "description": "MOST Web Framework Test Api Server",
  "repository": "https://github.com/themost-framework/themost-test.git",
  "author": "Kyriakos Barbounakis<k.barbounakis@gmail.com>",
  "main": "dist/server/index.js",
  "types": "dist/server/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/server",
    "build": "rimraf dist/server && babel server --out-dir dist/server --copy-files --source-maps",
    "serve": "nodemon server/standalone.js",
    "debug": "nodemon --inspect-brk server/standalone.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.14"
  },
  "dependencies": {
    "@themost/client": "^2.1.3",
    "@themost/common": "^2.3.1",
    "@themost/data": "^2.3.1",
    "@themost/express": "^1.3.3",
    "@themost/mem": "^1.0.8",
    "@themost/node": "^2.1.1",
    "@themost/query": "^2.3.1",
    "@themost/sqlite": "^2.2.1",
    "@themost/web": "^2.3.1",
    "@themost/xml": "^2.3.1",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "ejs-locals": "^1.0.2",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1"
  },
  "nodemonConfig": {
    "watch": [
      "server"
    ],
    "verbose": true,
    "legacy-watch": true,
    "execMap": {
      "js": "node --require @babel/register"
    },
    "env": {
      "NODE_ENV": "development",
      "PORT": 4000
    },
    "ext": "js,json"
  }
}
